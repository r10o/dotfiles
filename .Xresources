! vim: set filetype=xdefaults :
*foreground:	#ffffff
*background:	#151515

!black
*color0:		#101010
*color8:		#151515
!red
*color1:		#E84F4F
*color9:		#D23D3D
!green
*color2:		#B8D68C
*color10:		#A0CF5D
!yellow
*color3:		#E1AA5D
*color11:		#F39D21
!blue
*color4:		#7DC1CF
*color12:		#4E9FB1
!magenta
*color5:		#9B64FB
*color13:		#8542FF
!cyan
*color6:		#6D878D
*color14:		#42717B
!white
*color7:		#dddddd
*color15:		#ffffff

Xft.dpi:				96
Xft.antialias:			1
Xft.hinting:			0
Xft.hintstyle:			hintslight
Xft.lcdfilter:			lcddefault
Xft.rgba:				rgb

xterm*locale:			true
xterm*faceName:			DejaVuSansMono
xterm*faceSize:			9
xterm*borderWidth:		0
xterm*saveLines:		4096
