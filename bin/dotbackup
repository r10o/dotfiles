#!/bin/sh

rsync -va ~/.Xresources ~/backup/dotfiles
rsync -va ~/.Xmodmap ~/backup/dotfiles
rsync -va ~/.dircolors ~/backup/dotfiles
rsync -va ~/.vimrc ~/backup/dotfiles
rsync -va ~/.xinitrc ~/backup/dotfiles
rsync -va ~/.zprofile ~/backup/dotfiles
rsync -va ~/.zshrc ~/backup/dotfiles

rsync -va ~/bin/* --exclude 'cpanm' ~/backup/dotfiles/bin
rsync -va ~/.vim ~/backup/dotfiles
